@model routine_explorer.Models.Routine

@{
    ViewData["Title"] = "Create";
}
<h4>Upload Routine File</h4>

<div class="row">
    <div class="col-md-12">
        <fieldset style="margin-top: 15px">
            <legend>Upload an xlsx file</legend>
            <form method="post" asp-action="FileUpload" asp-controller="Routines" enctype="multipart/form-data">
                <input type="file" name="file" onchange="ValidateSingleInput(this)" />
                <input type="submit" value="Upload" />
            </form>
        </fieldset>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");
    await Html.RenderPartialAsync("_ValidateFileExtension");
    }
}
